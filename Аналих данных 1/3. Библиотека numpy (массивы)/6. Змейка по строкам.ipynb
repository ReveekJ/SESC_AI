      "source": [
        "import numpy as np\n",
        "\n",
        "a, b, n, m = map(int, input().split())\n",
        "mat = np.arange(a, b).reshape(n, m)\n",
        "for i in range(n):\n",
        "    if i % 2 == 1:\n",
        "        mat[i] = mat[i][::-1]\n",
        "\n",
        "print(mat)"
      ]
    }
