      "source": [
        "import numpy as np\n",
        "\n",
        "a, b, n, m = map(int, input().split())\n",
        "matrix = np.arange(a, b)[: n * m].reshape(m, n, order=\"F\")\n",
        "\n",
        "for j in range(n):\n",
        "    if j % 2 == 1:\n",
        "        matrix[:, j] = matrix[::-1, j]\n",
        "\n",
        "print(matrix)"
      ]
    }
