      "source": [
        "import numpy as np\n",
        "\n",
        "\n",
        "def diagonal_plus(matrix):\n",
        "    ma = matrix.max()\n",
        "    mi = matrix.min()\n",
        "    diags = np.diag_indices_from(matrix)\n",
        "    for i in diags[0]:\n",
        "        matrix[i, i] += ma - mi\n",
        "    return matrix"
      ]
    }
