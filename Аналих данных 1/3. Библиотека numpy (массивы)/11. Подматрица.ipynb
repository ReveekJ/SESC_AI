    "import numpy as np\n",
    "\n",
    "\n",
    "def sub_matrix(matrix, n, m):\n",
    "    if len(np.shape(matrix)) == 1:\n",
    "        return matrix[n:m]\n",
    "    elif len(np.shape(matrix)) == 2:\n",
    "        return matrix[n:m, n:m]\n",
    "    elif len(np.shape(matrix)) == 3:\n",
    "        return matrix[n:m, n:m, n:m]\n"
   ]
